{
   "name": "dnajs-node-jsdom-starter",
   "description": "Sample dna.js application running on Node.js with jsdom",
   "version": "1.0.0",
   "license": "MIT",
   "main": "app.js",
   "repository": "github:dnajs/dnajs-node-jsdom-starter",
   "homepage": "https://github.com/dnajs/dnajs-node-jsdom-starter",
   "bugs": "https://github.com/dnajs/dnajs-node-jsdom-starter/issues",
   "docs": "https://dnajs.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "node",
      "dna.js",
      "jsdom",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true
   },
   "scripts": {
      "test": "jshint *.js"
   },
   "dependencies": {
      "dna.js": "~1.5",
      "jquery": "~3.4",
      "jsdom": "~15.1",
      "jshint": "~2.10",
      "request": "~2.88"
   }
}
